{
  "name": "edrops-backend",
  "version": "0.0.1",
  "description": "edrops-backend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha \"dist/__tests__\"",
    "test:ci": "lb-mocha \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "precoverage": "npm test",
    "coverage": "open coverage/index.html",
    "coverage:ci": "lb-nyc report --reporter=text-lcov | coveralls",
    "docker:build": "docker build -t edrop_backend .",
    "docker:run": "docker run -p 3000:3000 -d edrop_backend",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "endpoints": "ts-node ./src/endpoints.ts"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Kenny477 <53405834+Kenny477@users.noreply.github.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^9.0.1",
    "@loopback/authorization": "^0.12.1",
    "@loopback/boot": "^5.0.1",
    "@loopback/booter-lb3app": "^4.0.1",
    "@loopback/core": "^4.0.1",
    "@loopback/repository": "^5.0.1",
    "@loopback/rest": "^12.0.1",
    "@loopback/rest-explorer": "^5.0.1",
    "@loopback/service-proxy": "^5.0.1",
    "@sendgrid/mail": "^7.7.0",
    "bluebird": "^3.7.2",
    "casbin": "^5.15.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "fs-extra": "^10.1.0",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "loopback": "^3.28.0",
    "loopback-boot": "^3.3.1",
    "loopback-component-explorer": "^6.5.1",
    "loopback-component-storage": "^3.7.0",
    "loopback-connector-mysql": "^6.1.0",
    "loopback-connector-rest": "^3.7.0",
    "loopback-connector-sendgrid": "^2.2.4",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.7.5",
    "serve-favicon": "^2.5.0",
    "shopify-buy": "^2.15.1",
    "strong-error-handler": "^4.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@loopback/build": "^9.0.1",
    "@loopback/eslint-config": "^13.0.1",
    "@loopback/testlab": "^5.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/ejs": "^3.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^14.18.22",
    "@types/node-fetch": "^2.6.2",
    "@types/shopify-buy": "^2.10.10",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-loopback": "^13.1.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "typescript": "~4.7.3"
  }
}
