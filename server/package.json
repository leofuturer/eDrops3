{
  "name": "@edroplets/backend",
  "version": "3.0.0",
  "description": "@edroplets/backend",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha \"dist/__tests__\"",
    "test:ci": "lb-mocha \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "precoverage": "npm test",
    "coverage": "open coverage/index.html",
    "coverage:ci": "lb-nyc report --reporter=text-lcov | coveralls",
    "docker:build": "docker build -t edrop_backend .",
    "docker:run": "docker run -p 3000:3000 -d edrop_backend",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "dev": "npm run rebuild && npm run start",
    "start:watch": "MIGRATE_DATABASE=Yes tsc-watch --onSuccess \"npm run dev\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "endpoints": "ts-node ./scripts/endpoints.ts"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Kenny477 <53405834+Kenny477@users.noreply.github.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^10.1.1",
    "@loopback/authorization": "^0.14.1",
    "@loopback/boot": "^6.1.1",
    "@loopback/core": "^5.0.0",
    "@loopback/repository": "^6.1.1",
    "@loopback/rest": "^13.1.1",
    "@loopback/rest-explorer": "^6.1.1",
    "@loopback/service-proxy": "^6.1.1",
    "@sendgrid/mail": "^7.7.0",
    "aws-sdk": "^2.1182.0",
    "bcryptjs": "^2.4.3",
    "casbin": "^5.15.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "loopback-connector-mysql": "^7.0.0",
    "loopback-connector-postgresql": "^6.0.6",
    "loopback-connector-rest": "^4.0.3",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-fetch": "^3.3.1",
    "pusher": "^5.1.1-beta",
    "serve-favicon": "^2.5.0",
    "shopify-buy": "^2.19.0",
    "strong-error-handler": "^4.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@loopback/build": "^10.1.1",
    "@loopback/eslint-config": "^14.0.2",
    "@loopback/testlab": "^6.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/ejs": "^3.1.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^20.4.2",
    "@types/node-fetch": "^2.6.2",
    "@types/shopify-buy": "^2.17.1",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-loopback": "^13.1.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.6.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  }
}
