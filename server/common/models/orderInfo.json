{
  "name": "orderInfo",
  "plural": "orderInfos",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "checkoutIdClient": {
      "type": "string",
      "required": true,
      "description": "Checkout ID generated by js-buy-sdk"
    },
    "checkoutToken": {
      "type": "string",
      "required": true,
      "description": "Checkout token generated by Shopify backend"
    },
    "checkoutLink": {
      "type": "string",
      "required": true,
      "description": "Checkout link generated by Shopify"
    },   
    "orderInfoId": {
      "type": "string",
      "required": false,
      "description": "Order ID generated by Shopify"
    },
    "createdAt": {
      "type": "string",
      "required": true,
      "description": "Time of order creation, as recorded by Shopify"
    },
    "lastModifiedAt": {
      "type": "string",
      "required": true,
      "description": "Time of last order modification"
    },
    "orderStatusURL": {
      "type": "string",
      "required": false
    },
    "orderComplete": {
      "type": "Boolean",
      "required": true,
      "description": "If false, represents current cart of customer"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "Order in progress"
    },
    "fees_and_taxes": {
      "type": "number",
      "required": false
    },
    "sa_name": {
      "type": "string",
      "required": false
    },
    "sa_address1": {
      "type": "string",
      "required": false,
      "description": "sa = shipping address"
    },
    "sa_address2": {
      "type": "string",
      "required": false
    },
    "sa_city": {
      "type": "string",
      "required": false
    },
    "sa_province": {
      "type": "string",
      "required": false
    },
    "sa_zip": {
      "type": "string",
      "required": false
    },
    "sa_country": {
      "type": "string",
      "required": false
    },
    "ba_name": {
      "type": "string",
      "required": false
    },
    "ba_address1": {
      "type": "string",
      "required": false,
      "description": "ba = billing address"
    },
    "ba_address2": {
      "type": "string",
      "required": false
    },
    "ba_city": {
      "type": "string",
      "required": false
    },
    "ba_province": {
      "type": "string",
      "required": false
    },
    "ba_zip": {
      "type": "string",
      "required": false
    },
    "ba_country": {
      "type": "string",
      "required": false
    },
    "otherDetails": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "customer",
      "foreignKey": "customerId",
      "options": {
        "nestRemoting": true
      }
    },
    "orderProducts": {
      "type": "hasMany",
      "model": "orderProduct",
      "foreignKey": "orderId",
      "options": {
        "nestRemoting": true
      }
    },
    "orderChips": {
      "type": "hasMany",
      "model": "orderChip",
      "foreignKey": "orderId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
