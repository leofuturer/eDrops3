{
  "name": "orderInfo",
  "plural": "orderInfos",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "checkoutIdClient": {
      "type": "string",
      "required": true,
      "description": "Checkout ID generated by js-buy-sdk"
    },
    "checkoutIdServer": {
      "type": "string",
      "required": true,
      "description": "Checkout ID generated by Shopify backend"
    },
    "orderInfoId": {
      "type": "string",
      "required": false,
      "description": "Order ID generated by Shopify"
    },
    "createdAt": {
      "type": "string",
      "required": true,
      "description": "Time of order creation, as recorded by Shopify"
    },
    "lastModifiedAt": {
      "type": "string",
      "required": true,
      "description": "Time of last order modification"
    },
    "orderStatusURL": {
      "type": "string",
      "required": false
    },
    "orderComplete": {
      "type": "Boolean",
      "required": true,
      "description": "If false, represents current cart of customer"
    },
    "totalCost": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "Order in progress"
    },
    "otherDetails": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "customer",
      "foreignKey": "customerId",
      "options": {
        "nestRemoting": true
      }
    },
    "shippingAddress": {
      "type": "belongsTo",
      "model": "customerAddress",
      "foreignKey": "shippingAddressId",
      "options": {
        "nestRemoting": true
      }
    },
    "billingAddress": {
      "type": "belongsTo",
      "model": "customerAddress",
      "foreignKey": "billingAddressId",
      "options": {
        "nestRemoting": true
      }
    },
    "orderProducts": {
      "type": "hasMany",
      "model": "orderProduct",
      "foreignKey": "orderId",
      "options": {
        "nestRemoting": true
      }
    },
    "orderChips": {
      "type": "hasMany",
      "model": "orderChip",
      "foreignKey": "orderId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
