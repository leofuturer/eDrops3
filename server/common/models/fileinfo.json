{
  "name": "fileInfo",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "uploadTime": {
      "type": "string",
      "required": true
    },
    "fileName": {
      "type": "string",
      "required": true
    },
    "containerFileName": {
      "type": "string",
      "required": true
    },
    "uploader": {
      "type": "string",
      "required": true
    },
    "fileSize": {
      "type": "string",
      "required": true
    },
    "isDeleted" : {
      "type": "Boolean",
      "required": true,
      "description": "Soft delete for files"
    },
    "container": {
      "type": "string",
      "required": true
    },
    "isPublic": {
        "type": "Boolean",
        "required": true
    },
    "units": {
        "type": "string",
        "required": true
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "customer",
      "foreignKey": "customerId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
