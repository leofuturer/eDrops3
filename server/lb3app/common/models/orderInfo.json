{
  "name": "orderInfo",
  "plural": "orderInfos",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "checkoutIdClient": {
      "type": "string",
      "required": true,
      "description": "Checkout ID generated by js-buy-sdk"
    },
    "checkoutToken": {
      "type": "string",
      "required": true,
      "description": "Checkout token generated by Shopify js-buy-sdk, used to tie initial order creation with webhooks"
    },
    "checkoutLink": {
      "type": "string",
      "required": true,
      "description": "Checkout link generated by Shopify"
    },
    "orderInfoId": {
      "type": "string",
      "required": false,
      "description": "Order ID generated by Shopify"
    },
    "createdAt": {
      "type": "string",
      "required": true,
      "description": "Time of order creation, as recorded by Shopify"
    },
    "lastModifiedAt": {
      "type": "string",
      "required": true,
      "description": "Time of last order modification"
    },
    "orderStatusURL": {
      "type": "string",
      "required": false
    },
    "orderComplete": {
      "type": "Boolean",
      "required": true,
      "description": "If false, represents current cart of customer"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "Order in progress"
    },
    "fees_and_taxes": {
      "type": "string",
      "required": false
    },
    "subtotal_cost": {
      "type": "string",
      "required": false
    },
    "total_cost": {
      "type": "string",
      "required": false
    },
    "user_email": {
      "type": "string",
      "required": false
    },
    "sa_name": {
      "type": "string",
      "required": false
    },
    "sa_address1": {
      "type": "string",
      "required": false,
      "description": "sa = shipping address"
    },
    "sa_address2": {
      "type": "string",
      "required": false
    },
    "sa_city": {
      "type": "string",
      "required": false
    },
    "sa_province": {
      "type": "string",
      "required": false
    },
    "sa_zip": {
      "type": "string",
      "required": false
    },
    "sa_country": {
      "type": "string",
      "required": false
    },
    "ba_name": {
      "type": "string",
      "required": false
    },
    "ba_address1": {
      "type": "string",
      "required": false,
      "description": "ba = billing address"
    },
    "ba_address2": {
      "type": "string",
      "required": false
    },
    "ba_city": {
      "type": "string",
      "required": false
    },
    "ba_province": {
      "type": "string",
      "required": false
    },
    "ba_zip": {
      "type": "string",
      "required": false
    },
    "ba_country": {
      "type": "string",
      "required": false
    },
    "otherDetails": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "customer",
      "foreignKey": "customerId",
      "options": {
        "nestRemoting": true
      }
    },
    "orderProducts": {
      "type": "hasMany",
      "model": "orderProduct",
      "foreignKey": "orderId",
      "options": {
        "nestRemoting": true
      }
    },
    "orderChips": {
      "type": "hasMany",
      "model": "orderChip",
      "foreignKey": "orderId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },

    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "findById"
    },

    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "addOrderProductToCart"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "addOrderChipToCart"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "newOrderCreated"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__get__orderProducts"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__get__orderChips"
    }
  ],
  "methods": {}
}
